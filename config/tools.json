{
  "tools": {
    "desktopCommander": {
      "command": "{npxPath}",
      "args": [
        "@wonderwhy-er/desktop-commander"
      ],
      "env": {
        "RIPGREP_PATH": "{ripgrepPath}"
      }
    },
    "git-mcp": {
      "command": "{uvxPath}",
      "args": [
        "mcp-server-git",
        "--repository",
        "{repoPath}"
      ]
    }
  }
}
