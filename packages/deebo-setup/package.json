{
  "name": "deebo-setup",
  "version": "1.0.102",
  "description": "System installer for Deebo debugging tool",
  "type": "module",
  "bin": {
    "deebo-setup": "build/index.js"
  },
  "exports": {
    "./guide-server": "./build/guide-server.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-guide-markdown",
    "copy-guide-markdown": "copyfiles -u 1 src/deebo_guide.md build",
    "start": "node build/index.js"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "inquirer": "^9.2.16",
    "simple-git": "^3.22.0",
    "zod": "^3.22.4"
  },
  "files": [
    "build/**/*"
  ],
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.28",
    "copyfiles": "^2.4.1",
    "typescript": "^5.4.2"
  }
}
